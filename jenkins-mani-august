pipeline{

    agent any

    stages{
        stage("Git clone"){
            steps{
                git 'https://github.com/manikantak2762/myweb.git'
            }
        }
        stage("Clean"){
            steps{
                sh "mvn clean"
            }
        }
        stage("Test"){
            steps{
                sh "mvn test"
            }
        }
        stage("Build"){
            sh "mvn package"
            sh "mv target/*.ear target/myweb.war"
        }
    }
}